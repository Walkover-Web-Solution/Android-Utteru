<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/white"
    android:paddingBottom="@dimen/padding_bottom_em"
    android:paddingTop="@dimen/padding_top_em"
    android:paddingLeft="@dimen/new_margin"
    android:paddingRight="@dimen/new_margin"
    >

    <com.Utteru.commonUtilities.FontTextView
        android:id="@+id/call_times"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:text="21 sep, 10.30AM"
        android:layout_marginLeft="10dp"
        android:textColor="@color/new_black"
        android:textSize="20dp" />

    <com.Utteru.commonUtilities.FontTextView
        android:id="@+id/call_durations"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/call_times"
        android:layout_marginTop="@dimen/margin_top_grid"
        android:text="00:55"
        android:layout_marginLeft="10dp"
        android:textColor="@color/dark_gray" />
    <com.Utteru.commonUtilities.FontTextView
        android:id="@+id/call_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/transparent_black"

        android:layout_below="@id/call_times"
        android:layout_toRightOf="@id/call_durations"
        android:layout_marginLeft="@dimen/new_margin"
        android:layout_marginTop="@dimen/margin_top_grid"
        android:paddingTop="1dp"
        android:paddingBottom="1dp"
        android:paddingRight="2dp"
        android:paddingLeft="2dp"


        android:textColor="@android:color/white" />


</RelativeLayout><!-- From: file:/C:/Studio_Projects/utteru/app/src/main/res/layout/dialer_recent_detailed_row.xml -->