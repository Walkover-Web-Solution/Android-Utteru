<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white">

    <!-- Use standard android.R class list id instead of app specific id. This is just useful for
         consistency. -->

    <android.support.v4.widget.SwipeRefreshLayout
        android:layout_width="match_parent"
        android:id="@+id/swipeRefreshLayout"
        android:layout_height="match_parent">

    <ListView
        android:id="@id/android:list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@drawable/horizontal_seperator"
        />
    </android.support.v4.widget.SwipeRefreshLayout>


    <!-- Use standard android.R class empty id instead of app specific id. This is just useful for
     consistency. -->


    <com.Utteru.commonUtilities.FontTextView
        android:id="@id/android:empty"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fontFamily="sans-serif-light"
        android:gravity="center"
        android:text="@string/no_aceess_number_found"
        android:textAppearance="?android:attr/textAppearanceLarge" />
    <LinearLayout

        android:id="@+id/dialpad_layout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_gravity="bottom"
        android:background="@android:color/transparent"
        android:visibility="gone"
        android:orientation="vertical"
        >

        <android.inputmethodservice.KeyboardView
            android:id="@+id/keyboardview"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@color/keyboard_bg"
            android:keyBackground="@drawable/key_background"
            android:keyTextColor="#666666"
            android:keyTextSize="35dp"
            android:shadowColor="@android:color/transparent" />


    </LinearLayout>

</FrameLayout>
